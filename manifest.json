{
	"manifest_version": 2,

	"name": "testtest",
	"description": "what can plugins do?",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["jquery.min.js", "background.js"]
	},
	"permissions": [
		"tabs",  "https://www.gateway.playneverwinter.com/"
		//"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery.min.js", "main.js"]
		}
	]
}
